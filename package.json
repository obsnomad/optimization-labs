{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 10.13 <11"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.14.0",
    "console.table": "^0.10.0",
    "jest": "~24.8.0",
    "prettier": "~1.17.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "~6.1.0",
    "tsutils": "~3.10.0",
    "typescript": "~3.4.0"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "Apache-2.0",
  "dependencies": {
    "es-iter": "^1.1.1",
    "fraction.js": "^4.0.12",
    "mathjs": "^6.2.5",
    "moment": "^2.24.0",
    "tslib": "~1.9.3"
  },
  "main": "config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obsnomad/optimization-labs.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/obsnomad/optimization-labs/issues"
  },
